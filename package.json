{
  "name": "hono-idempotent-request",
  "type": "module",
  "version": "0.0.1",
  "description": "Hono middleware for idempotent request",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./brand": {
      "types": "./dist/brand/index.d.ts",
      "default": "./dist/brand/index.mjs"
    },
    "./server-specification": {
      "types": "./dist/server-specification/index.d.ts",
      "default": "./dist/server-specification/index.mjs"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "default": "./dist/storage/index.mjs"
    }
  },
  "keywords": [
    "hono",
    "middleware",
    "Idempotency-Key"
  ],
  "author": "sushichan044 <mail@sushichan.live> (https://github.com/sushichan044)",
  "license": "MIT",
  "packageManager": "pnpm@10.6.5",
  "peerDependencies": {
    "hono": "^3.8.0 || ^4.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint --max-warnings 0",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "test": "vitest",
    "test:run": "vitest --run",
    "typecheck": "tsc --noEmit",
    "check": "pnpm run lint && pnpm run format:ci && pnpm run typecheck && pnpm run test:run && pnpm run build"
  },
  "devDependencies": {
    "@hono/standard-validator": "0.1.2",
    "@oslojs/crypto": "1.0.1",
    "@oslojs/encoding": "1.1.0",
    "@types/node": "22.13.10",
    "@virtual-live-lab/eslint-config": "2.2.18",
    "@virtual-live-lab/prettier-config": "2.0.17",
    "@virtual-live-lab/tsconfig": "2.1.18",
    "@vitest/coverage-v8": "3.0.9",
    "eslint": "9.22.0",
    "eslint-plugin-unicorn": "57.0.0",
    "globals": "16.0.0",
    "hono": "4.7.4",
    "prettier": "3.5.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.27.0",
    "unbuild": "3.5.0",
    "uuid": "11.1.0",
    "valibot": "1.0.0",
    "vitest": "3.0.9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
