{
  "name": "hono-idempotent-request",
  "type": "module",
  "version": "0.0.1",
  "description": "Hono middleware for idempotent request",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sushichan044/hono-idempotent-request.git"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "keywords": [
    "hono",
    "middleware",
    "Idempotency-Key"
  ],
  "author": "sushichan044 (https://github.com/sushichan044)",
  "license": "MIT",
  "packageManager": "pnpm@10.6.5",
  "scripts": {
    "build": "unbuild",
    "lint": "eslint --max-warnings 0",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "test": "vitest",
    "test:run": "vitest --run",
    "typecheck": "tsc --noEmit",
    "check": "pnpm run lint && pnpm run format:ci && pnpm run typecheck && pnpm run test:run && pnpm run build"
  },
  "dependencies": {
    "@universal-middleware/core": "0.4.7"
  },
  "devDependencies": {
    "@types/node": "22.14.1",
    "@virtual-live-lab/eslint-config": "2.2.21",
    "@virtual-live-lab/prettier-config": "2.0.19",
    "@virtual-live-lab/tsconfig": "2.1.20",
    "@vitest/coverage-v8": "3.0.9",
    "eslint": "9.25.0",
    "eslint-plugin-unicorn": "58.0.0",
    "globals": "16.0.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.30.1",
    "unbuild": "3.5.0",
    "vitest": "3.0.9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
